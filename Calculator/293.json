        "logic": "var calc_output = [];\n\nvar incomplete = parseFloat(incomplete);\nvar freq = parseFloat(freq);\nvar intermit = parseFloat(intermit);\nvar urgency = parseFloat(urgency);\nvar weak = parseFloat(weak);\nvar strain = parseFloat(strain);\nvar noct = parseFloat(noct);\n\nvar quality_score = parseFloat(quality);\n\nvar score = incomplete + freq + intermit + urgency + weak + strain + noct;\n\nvar severity;\n\nif (score <= 7) {\n    severity = 'Mild';\n} else if (score <= 19) {\n    severity = 'Moderate'\n} else {\n    severity = 'Severe'\n}\n\ncalc_output.push({\n    name: 'mini',\n    value: score,\n    value_text: 'points',\n    message: 'AUA-SI/IPSS Score'\n});\ncalc_output.push({\n    name: 'AUA-SI/IPSS Score',\n    value: score,\n    value_text: 'points',\n    message: 'AUA-SI/IPSS Score'\n});\ncalc_output.push({\n    name: 'Severity',\n    value: severity,\n    value_text: '',\n    message: 'BPH symptom severity'\n});\n\nif (quality_score > 0) {\n    var quality;\n    if (quality_score == 1) {\n        quality = 'Delighted';\n    } else if (quality_score == 2) {\n        quality = 'Pleased';\n    } else if (quality_score == 3) {\n        quality = 'Mostly satisfied';\n    } else if (quality_score == 4) {\n        quality = 'Mixed';\n    } else if (quality_score == 5) {\n        quality = 'Mostly dissatisfied';\n    } else if (quality_score == 6) {\n        quality = 'Unhappy';\n    } else {\n        quality = 'Terrible';\n    }\n\n    calc_output.push({\n        name: 'Quality',\n        value: quality,\n        value_text: '',\n        message: 'Quality of life'\n    });\n}",
