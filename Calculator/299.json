        ],
        "evidence": "<p dir=\"ltr\"><span>The STUMBL/Battle Score is calculated by the addition of the selected points:</span><b></b></p>\n<div dir=\"ltr\" align=\"left\">\n<table style=\"width: 100%;\"><colgroup><col style=\"width: 41.4352%;\"><col style=\"width: 20.8403%;\"><col style=\"width: 37.7245%;\"></colgroup>\n<tbody>\n<tr>\n<td colspan=\"2\">\n<p dir=\"ltr\"><strong>Variable</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Points</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>Age, years</span></p>\n</td>\n<td></td>\n<td>\n<p dir=\"ltr\"><span>1 point per 10 years</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>Number of rib fractures</span></p>\n</td>\n<td></td>\n<td>\n<p dir=\"ltr\"><span>3 points per rib fracture</span></p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"2\">\n<p dir=\"ltr\"><span>Chronic lung disease</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>No</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>0</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>Yes</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>5</span></p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"2\">\n<p dir=\"ltr\"><span>Pre-injury anticoagulant use</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>No</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>0</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>Yes</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>4</span></p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"4\">\n<p dir=\"ltr\"><span>Oxygen saturation level</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>&ge;95%</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>0</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>90-94%</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>2</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>85-89%</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>4</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>80-84%</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>6</span></p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p></p>",
        "facts": "<p dir=\"ltr\"><span>Interpretation:</span><b></b></p>\n<div dir=\"ltr\" align=\"left\">\n<table><colgroup><col><col></colgroup>\n<tbody>\n<tr>\n<td>\n<p dir=\"ltr\"><strong>STUMBL/Battle Score</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Risk of Complications</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>0-10</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>13 &plusmn; 6%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>11-15</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>29 &plusmn; 8%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>16-20</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>52 &plusmn; 8%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>21-25</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>70 &plusmn; 6%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>26-30</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>80 &plusmn; 6%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>&ge;31</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>88 &plusmn; 7%</span></p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p></p>",
