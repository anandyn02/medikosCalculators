        "logic": "var calc_output = [];\n\nvar dvt = parseFloat(dvt);\nvar hemoptysis = parseFloat(hemoptysis);\nvar likely = parseFloat(likely);\nvar dimer500 = parseFloat(dimer500);\nvar dimer1000 = parseFloat(dimer1000);\nvar pregnant = parseFloat(pregnant);\nvar comp = parseFloat(comp);\nvar pregnant = parseFloat(pregnant);\n\nvar msg, result;\n\nif (dvt + hemoptysis + likely == 0) {\n\tif (dimer1000 == 0) {\n\t\tresult = 'PE excluded'\n\t\tmsg = 'YEARS algorithm rules out PE (0.43% with symptomatic VTE during 3-month follow-up)'\n    } else {\n\t\tresult = 'PE not excluded'\n\t\tmsg = 'YEARS algorithm recommends CTPA'\n\t}\n}\nelse {\n  if (dvt == 1 && pregnant == 1 && comp == 0) {\n    result = 'Initiate anticoagulant'\n    msg = 'Pregnancy-adapted YEARS algorithm recommends initiation of anticoagulant treatment'\n  } else if (dimer500 == 0) {\n\t\tresult = 'PE excluded'\n\t\tmsg = 'YEARS algorithm rules out PE (0.43% with symptomatic VTE during 3-month follow-up)'\n  } else {\n\t\tresult = 'PE not excluded'\n\t\tmsg = 'YEARS algorithm recommends CTPA'\n\t}\n}\n\n//results\ncalc_output.push({\n    name: 'mini',\n    value: result,\n    value_text: '',\n    message: ''\n});\ncalc_output.push({\n    name: '4067_YEARS Algorithm_result',\n    value: result,\n    value_text: '',\n    message: msg\n});",
