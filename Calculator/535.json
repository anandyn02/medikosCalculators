        "logic": "var calc_output = [];\n\nvar u = parseFloat(ichvol) + parseFloat(age) + parseFloat(location) + parseFloat(gcs) + parseFloat(cog);\n\nvar msg;\n\nif (u <= 4) {\n    msg = 'Entire cohort: 0% probability of functional independence at 90 days<br/>Survivors only: 0% probability of functional independence at 90 days';\n} else if (u <= 7) {\n    msg = 'Entire cohort: 13% probability of functional independence at 90 days<br/>Survivors only: 29% probability of functional independence at 90 days';\n} else if (u == 8) {\n    msg = 'Entire cohort: 42% probability of functional independence at 90 days<br/>Survivors only: 48% probability of functional independence at 90 days';\n} else if (u <= 10) {\n    msg = 'Entire cohort: 66% probability of functional independence at 90 days<br/>Survivors only: 75% probability of functional independence at 90 days';\n} else {\n    msg = 'Entire cohort: 82% probability of functional independence at 90 days<br/>Survivors only: 95% probability of functional independence at 90 days';\n}\n\ncalc_output.push({\n    name: 'mini',\n    value: u,\n    value_text: 'points',\n    message: msg\n});\ncalc_output.push({\n    name: '3883_FUNC Score_result',\n    value: u,\n    value_text: 'points',\n    message: msg\n});",
