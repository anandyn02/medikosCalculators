        "logic": "var calc_output=[];\n\nvar severity = parseFloat(severity);\n\nif (severity <2) {\n  var priest = parseFloat(sex) + parseFloat(age) + parseFloat(res) + parseFloat(oxy) + parseFloat(bpm) + parseFloat(sys) + parseFloat(temp) + parseFloat(alert) + parseFloat(ins) + parseFloat(status);\n\nvar risk = parseFloat(risk1) + parseFloat(risk2) + parseFloat(risk3) + parseFloat(risk4) + parseFloat(risk5) + parseFloat(risk6) + parseFloat(risk7) + parseFloat(risk8) + parseFloat(risk9) + parseFloat(risk10) + parseFloat(risk11) + parseFloat(risk12) + parseFloat(risk13) + parseFloat(risk14) + parseFloat(risk15) + parseFloat(risk16);\n}\n\nvar result;\nvar wkup;\nvar tx;\nvar admit;\nvar a = '';\nvar b = '';\n\n\nif (priest <=1){\n  b = '1';\n}\n\nif (priest >=2 && priest <=3){\n  b = '2';\n}\n\nif (priest == 4){\n  b = '3';\n}\n\nif (priest == 5){\n  b = '9';\n}\n\nif (priest == 6){\n  b = '15';\n}\n\nif (priest == 7){\n  b = '18';\n}\n\nif (priest == 8){\n  b = '22';\n}\n\nif (priest == 9){\n  b = '26';\n}\n\nif (priest == 10){\n  b = '29';\n}\n\nif (priest == 11){\n  b = '34';\n}\n\nif (priest == 12){\n  b = '38';\n}\n\nif (priest == 13){\n  b = '47';\n}\n\nif (priest == 14){\n  b = '48';\n}\n\nif (priest == 15){\n  b = '50';\n}\n\nif (priest == 16){\n  b = '55';\n}\n\nif (priest >=17){\n  b = '66';\n}\n\n\nif (severity == 0) {\n  result = 'Mild';\n  if (priest <= 4 && risk <= 1) {\n    a = 'PRIEST Score = ' + priest + ' points PLUS ' + risk + ' additional risk factors, ' +  b + '% 30-day probability of adverse outcome (death or organ support)<br/><br/>';\n    wkup = 'Workup: Usually not necessary';\n    admit = 'Disposition: D/C home with CDC patient materials + consider home PulseOx';\n    tx = 'Treatment: Likely candidate for IV Ab or oral antivirals (if early in diagnosis; some risk factors) - follow NIH guidelines and local protocol';\n  } else {\n    a = 'PRIEST Score = ' + priest + ' points PLUS ' + risk + ' additional risk factors, ' +  b + '% 30-day probability of adverse outcome (death or organ support)<br/><br/>';\n    wkup = 'Workup: Usually not necessary';\n    admit = 'Disposition: D/C home with CDC patient materials<br/>Close f/u to PMD + consider home PulseOx + risk of ED return';\n    tx = 'Treatment: Likely candidate for IV Ab or oral antivirals (if early in diagnosis; some risk factors) - follow NIH guidelines and local protocol';\n  }\n} else if (severity == 1) {\n  result = 'Moderate';\n  if (priest <= 4 && risk <= 1) {\n    a = 'PRIEST Score = ' + priest + ' points PLUS ' + risk + ' additional risk factors, ' +  b + '% 30-day probability of adverse outcome (death or organ support)<br/><br/>';\n    wkup = 'Workup: Consider imaging, labs, ECG';\n    admit = 'Disposition: Consider discharge if no concerning workup and patient has close follow-up<br/>Admit if other condition warrants';\n    tx = 'Treatment: If discharged, likely candidate for IV Ab or oral antivirals (if early in diagnosis; some risk factors) - follow NIH guidelines and local protocol<br/>If admitted, unlikely to be a candidate for IV ab and oral antivirals';\n  } else {\n    a = 'PRIEST Score = ' + priest + ' points PLUS ' + risk + ' additional risk factors, ' +  b + '% 30-day probability of adverse outcome (death or organ support)<br/><br/>';\n    wkup = 'Workup: Consider imaging, labs, ECG';\n    admit = 'Disposition: Admit to appropriate level of care';\n    tx = 'Treatment: Consider remdesivir if high risk of progression, unlikely candidate for IV Ab or po antivirals - follow NIH guidelines and local protocol';\n  }\n} else if (severity == 2) {\n  result = 'Severe';\n  wkup = 'Workup: Imaging, labs, ECG';\n  admit = 'Disposition: Clinical decision floor vs intermediate vs ICU; consider transfer if low resources';\n  tx = 'Treatment: Oxygen support, steroids, remdesivir +/- toclizumab<br/>Unlikely candidate for IV Ab or oral antivirals<br/>Follow NIH guidelines and local protocols';\n} else if (severity == 3) {\n  result = 'Critical';\n  wkup = 'Workup: Imaging, labs, ECG';\n  admit = 'Disposition: ICU; consider transfer if low resources, ECMO consult';\n  tx = 'Treatment: Respiratory and oxygen support, steroids, remdesivir, tocilizumab<br/>Unlikely candidate for IV or oral antivirals<br/>Follow NIH guidelines and local protocols';\n} \n\nvar msg = a + wkup + '<br/>' + '<br/>' + admit + '<br/>' + '<br/>' + tx;\n\ncalc_output.push({\n    name: 'mini',\n    value: result,\n    value_text:'',\n    message: 'Click to see recs.'\n});\ncalc_output.push({\n    name: 'outcome',\n    value: result,\n    value_text:'',\n    message: msg\n});",
