        ],
        "evidence": "<p dir=\"ltr\">Selection of the appropriate criteria:</p>\n<div dir=\"ltr\">\n<table><colgroup><col width=\"208\"></colgroup>\n<tbody>\n<tr>\n<td>\n<p dir=\"ltr\"><strong>Criteria</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Age &ge;60 years</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><abbr title=\"Chronic heart failure\">CHF</abbr> history</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Smoking history</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Hemoptysis</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Tuberculosis history</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Thromboembolic history</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Prior/current alcohol abuse</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Fever &ge;100.4&deg;F (38&deg;C)</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Oxygen saturation &lt;90%</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Respiratory rate &gt;24</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Diminished breath sounds</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Rales</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p><span id=\"docs-internal-guid-c4e4e062-108c-7177-a9ed-7a8835783999\"></span><span id=\"docs-internal-guid-c4e4e062-108c-7177-a9ed-7a8835783999\"><br></span></p>",
        "facts": "<p dir=\"ltr\">Interpretation:</p>\n<div dir=\"ltr\">\n<table><colgroup><col width=\"66\"><col width=\"118\"><col width=\"187\"></colgroup>\n<tbody>\n<tr>\n<td>\n<p dir=\"ltr\"><strong>Criteria present</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Risk group</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Recommendation</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n<td>\n<p dir=\"ltr\">Low</p>\n</td>\n<td>\n<p dir=\"ltr\">Chest x-ray not needed</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&gt;0</p>\n</td>\n<td>\n<p dir=\"ltr\">Not low</p>\n</td>\n<td>\n<p dir=\"ltr\">Consider chest x-ray</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p><span id=\"docs-internal-guid-d4e476a7-108c-edb0-3730-1a088d5e615d\"></span><span id=\"docs-internal-guid-d4e476a7-108c-edb0-3730-1a088d5e615d\"><br></span></p>",
        "estimation": [],
        "primary_complains": [
            "Chest Pain"
        ],
        "disease_treated": [
            "Congestive Heart Failure",
            "Heart Failure",
            "Respiratory Distress"
        ],
        "logic": "var calc_output = [];\nvar score = parseFloat(rales) + parseFloat(breath) + parseFloat(respiratory) + parseFloat(o2) + parseFloat(fever) + parseFloat(alc) + parseFloat(thrombo) + parseFloat(tuberculosis) + parseFloat(fever) + parseFloat(hemoptysis) + parseFloat(smoking) + parseFloat(chf) + parseFloat(age);\nvar msg;\nvar risk;\nif (score == 0) {\n    risk = 'Low risk';\n    msg = 'Chest x-ray not necessary';\n} else if (score > 0) {\n    risk = 'Not low risk';\n    msg = 'Consider chest x-ray';\n}\ncalc_output.push({ name: 'mini', value: risk, value_text: '', message: msg });\n\ncalc_output.push({ name: 'Risk', value: risk, value_text: '', message: msg });",
