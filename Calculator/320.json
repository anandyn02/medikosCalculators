        "facts": "<p dir=\"ltr\"><span>Interpretation:</span><b></b></p>\n<div dir=\"ltr\" align=\"left\">\n<table style=\"width: 64.4918%;\"><colgroup><col style=\"width: 37.8855%;\"><col style=\"width: 62.1145%;\"></colgroup>\n<tbody>\n<tr>\n<td>\n<p dir=\"ltr\"><strong>Score</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Probability of brain metastasis</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>0-79</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>&lt;4%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>80-89</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>4%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>90-124</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>5%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>125-162</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>10%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>163-187</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>15%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>188-212</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>21%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>213-237</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>27%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>238-262</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>38%</span></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\"><span>263-293</span></p>\n</td>\n<td>\n<p dir=\"ltr\"><span>47%</span></p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p></p>",
