        ],
        "evidence": "<p dir=\"ltr\">Addition of the selected points:</p>\n<div dir=\"ltr\">\n<table><colgroup><col width=\"261\" /><col width=\"70\" /><col width=\"63\" /></colgroup>\n<tbody>\n<tr>\n<td colspan=\"2\">\n<p dir=\"ltr\"><strong>Finding</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Points</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Loss of consciousness or syncope</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">-1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Seizure activity</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">-1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td colspan=\"3\">\n<p dir=\"ltr\"><strong>New, acute onset (or upon awaking from sleep) finding of any of the following:</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Asymmetric facial weakness</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">+1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Asymmetric arm weakness</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">+1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Asymmetric leg weakness</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">+1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Speech disturbance</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">+1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Visual field defect</p>\n</td>\n<td>\n<p dir=\"ltr\">Yes</p>\n<p dir=\"ltr\">No</p>\n</td>\n<td>\n<p dir=\"ltr\">+1</p>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>",
        "facts": "<p dir=\"ltr\">Likelihood of stroke:</p>\n<p><span id=\"docs-internal-guid-c82a8761-ffc6-4cad-18c9-172359f2356e\"></span></p>\n<div dir=\"ltr\">\n<table><colgroup><col width=\"112\" /><col width=\"126\" /></colgroup>\n<tbody>\n<tr>\n<td>\n<p dir=\"ltr\"><strong>ROSIER Score</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Interpretation</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&le;0</p>\n</td>\n<td>\n<p dir=\"ltr\">Stroke unlikely</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&gt;0</p>\n</td>\n<td>\n<p dir=\"ltr\">Stroke possible</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>",
        "estimation": [],
        "primary_complains": [
            "Weakness",
