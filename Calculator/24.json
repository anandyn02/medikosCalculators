        ],
        "disease_treated": [
            "Stroke/TIA"
        ],
        "logic": "var calc_output = [];\n\nvar syncope = parseFloat(syncope)\nvar seizure = parseFloat(seizure)\nvar facialweakness = parseFloat(facialweakness)\nvar armweakness = parseFloat(armweakness)\nvar legweakness = parseFloat(legweakness)\nvar speech = parseFloat(speech)\nvar visualdefect = parseFloat(visualdefect)\n\nvar score = syncope + seizure + facialweakness + armweakness + legweakness + speech + visualdefect \n\nvar interpretation\n\n\nif (score <= 0) {\n    interpretation = 'Stroke unlikely' \n}    \nelse if (score > 0) { \n    interpretation = 'Stroke possible'\n}\n\n//results \ncalc_output.push({\n    name: 'mini',\n    value: interpretation,\n    value_text: '',\n    message: score + ' points' \n    });\ncalc_output.push({\n    name: 'Score',\n    value: score,\n    value_text: 'points',\n    message: 'ROSIER Score'\n    });\ncalc_output.push({\n    name: 'interpretation',\n    value: interpretation, \n    value_text: '',\n    message: ''\n});",
