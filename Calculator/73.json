        "evidence": "<p dir=\"ltr\">Addition of the selected points:</p>\n<div dir=\"ltr\">\n<table><colgroup><col width=\"170\"><col width=\"170\"><col width=\"97\"></colgroup>\n<tbody>\n<tr>\n<td colspan=\"2\">\n<p dir=\"ltr\"><strong>Variable</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Points</strong></p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"5\">\n<p dir=\"ltr\">Respiratory rate, breaths per minute</p>\n</td>\n<td>\n<p dir=\"ltr\">&le;8</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">9-11</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">12-20</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">21-24</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;25</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"4\">\n<p dir=\"ltr\">SpO<sub>2</sub> (on room air or supplemental)</p>\n</td>\n<td>\n<p dir=\"ltr\">&le;91%</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">92-93%</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">94-95%</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;96%</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"7\">\n<p dir=\"ltr\">SpO<sub>2</sub> (if patient has hypercapnic respiratory failure)</p>\n</td>\n<td>\n<p dir=\"ltr\">&le;83%</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">84-85%</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">86-87%</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">88-92%, &ge;93% on room air</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">93-94% on supplemental oxygen</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">95-96% on supplemental oxygen</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;97% on supplemental oxygen</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"2\">\n<p dir=\"ltr\">Room air or supplemental oxygen</p>\n</td>\n<td>\n<p dir=\"ltr\">Supplemental oxygen</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Room air</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"5\">\n<p dir=\"ltr\">Temperature</p>\n</td>\n<td>\n<p dir=\"ltr\">&le;35.0&deg;C (95&deg;F)</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">35.1-36.0&deg;C (95.1-96.8&deg;F)</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">36.1-38.0&deg;C (96.9-100.4&deg;F)</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">38.1-39.0&deg;C (100.5-102.2&deg;F)</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;39.1&deg;C (102.3&deg;F)</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"5\">\n<p dir=\"ltr\">Systolic BP, mmHg</p>\n</td>\n<td>\n<p dir=\"ltr\">&le;90</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">91-100</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">101-110</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">111-219</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;220</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"6\">\n<p dir=\"ltr\">Pulse, beats per minute</p>\n</td>\n<td>\n<p dir=\"ltr\">&le;40</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">41-50</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">51-90</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">91-110</p>\n</td>\n<td>\n<p dir=\"ltr\">1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">111-130</p>\n</td>\n<td>\n<p dir=\"ltr\">2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;131</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"2\">\n<p dir=\"ltr\">Consciousness</p>\n</td>\n<td>\n<p dir=\"ltr\">Alert</p>\n</td>\n<td>\n<p dir=\"ltr\">0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">New-onset confusion (or disorientation/agitation), responds to voice, responds to pain, or unresponsive</p>\n</td>\n<td>\n<p dir=\"ltr\">3</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p><span id=\"docs-internal-guid-a799da09-7fff-3b85-8bf8-fe305cf40dfd\"><span id=\"docs-internal-guid-a799da09-7fff-3b85-8bf8-fe305cf40dfd\">&nbsp;</span></span></p>\n<p dir=\"ltr\"></p>",
        "facts": "<p dir=\"ltr\">Interpretation:</p>\n<div dir=\"ltr\">\n<table><colgroup><col width=\"*\"><col width=\"93\"><col width=\"205\"></colgroup>\n<tbody>\n<tr>\n<td>\n<p dir=\"ltr\"><strong>NEW Score</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Clinical risk</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Frequency of monitoring</strong></p>\n</td>\n<td>\n<p dir=\"ltr\"><strong>Response</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">0-4</p>\n</td>\n<td>\n<p dir=\"ltr\">Low</p>\n</td>\n<td>\n<p dir=\"ltr\">Minimum every 12 hrs if score of 0</p>\n<p dir=\"ltr\">Minimum every 4-6 hrs if score 1-4</p>\n</td>\n<td>\n<p dir=\"ltr\">Assessment by a competent registered nurse or equivalent, to decide change in frequency of clinical monitoring or escalation of care</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">Score of 3 in any individual parameter</p>\n</td>\n<td>\n<p dir=\"ltr\">Low-medium</p>\n</td>\n<td rowspan=\"2\">Minimum every hr\n<p dir=\"ltr\"></p>\n</td>\n<td>\n<p dir=\"ltr\">Urgent review by a ward-based doctor, to decide change in frequency of clinical monitoring or escalation of care</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">5-6</p>\n</td>\n<td>\n<p dir=\"ltr\">Medium</p>\n</td>\n<td>\n<p dir=\"ltr\">Urgent review by a ward-based doctor or acute team nurse, to decide if critical care team assessment is needed</p>\n</td>\n</tr>\n<tr>\n<td>\n<p dir=\"ltr\">&ge;7</p>\n</td>\n<td>\n<p dir=\"ltr\">High</p>\n</td>\n<td>\n<p dir=\"ltr\">Continuous monitoring of vital signs</p>\n</td>\n<td>\n<p dir=\"ltr\">Emergent assessment by a clinical team or critical care team and usually transfer to higher level of care</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>\n<p><span id=\"docs-internal-guid-b962a8c3-7fff-c2e9-c37c-6ecbbdc1a99c\"></span><span id=\"docs-internal-guid-b962a8c3-7fff-c2e9-c37c-6ecbbdc1a99c\"><br></span></p>",
