        ],
        "evidence": "<p>Addition of selected points:</p>\n<div class=\"table-responsive\">\n<table class=\"table table-bordered\">\n<tbody>\n<tr>\n<td rowspan=\"2\">\n<p><strong>Vomiting</strong></p>\n</td>\n<td>\n<p>No</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>Yes</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"2\">\n<p><strong>RIF pain</strong></p>\n</td>\n<td>\n<p>No</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>Yes</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"4\">\n<p><strong>Rebound tenderness </strong></p>\n</td>\n<td>\n<p>None</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>Light</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>Medium</p>\n</td>\n<td>\n<p>+2</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>Strong</p>\n</td>\n<td>\n<p>+3</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"2\">\n<p><strong>Temp </strong><strong>&ge;101.3&ordm;F (38.5&ordm;C)</strong></p>\n</td>\n<td>\n<p>No</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>Yes</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"3\">\n<p><strong>Polymorphonuclear leukocytes</strong></p>\n</td>\n<td>\n<p>&lt;70%</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>70&ndash;84%</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>&ge;85%</p>\n</td>\n<td>\n<p>+2</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"3\">\n<p><strong>WBC count, &times;10<sup>9</sup></strong><strong>/L</strong></p>\n</td>\n<td>\n<p>&lt;10</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>10.0&ndash;14.9</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>&ge;15</p>\n</td>\n<td>\n<p>+2</p>\n</td>\n</tr>\n<tr>\n<td rowspan=\"3\">\n<p><strong>CRP level, mg/L</strong></p>\n</td>\n<td>\n<p>&lt;10</p>\n</td>\n<td>\n<p>0</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>10&ndash;49</p>\n</td>\n<td>\n<p>+1</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>&ge;50</p>\n</td>\n<td>\n<p>+2</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>",
        "facts": "<p>Interpretation:</p>\n<div class=\"table-responsive\">\n<table class=\"table table-bordered\">\n<tbody>\n<tr>\n<td>\n<p><strong>AIR Score</strong></p>\n</td>\n<td>\n<p><strong>Risk</strong></p>\n</td>\n<td>\n<p><strong>Recommendation</strong></p>\n</td>\n</tr>\n<tr>\n<td>\n<p>0&ndash;4</p>\n</td>\n<td>\n<p>Low</p>\n</td>\n<td>\n<p>Outpatient follow-up (if unaltered general condition)</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>5&ndash;8</p>\n</td>\n<td>\n<p>Indeterminate</p>\n</td>\n<td>\n<p>In-hospital active observation with serial re-exams, imaging, or diagnostic laparoscopy, according to local practice</p>\n</td>\n</tr>\n<tr>\n<td>\n<p>9&ndash;12</p>\n</td>\n<td>\n<p>High</p>\n</td>\n<td>\n<p>Surgical exploration</p>\n</td>\n</tr>\n</tbody>\n</table>\n</div>",
        "estimation": [],
        "primary_complains": [
            "Abdominal Pain",
            "Pain",
            "Vomiting"
        ],
        "disease_treated": [
            "Appendicitis"
        ],
        "logic": "var calc_output = [];\n\n//user inputs - addition of selected points\nvar score = parseFloat(vomit) + parseFloat(pain) + parseFloat(rebound) + parseFloat(body) + parseFloat(poly) + parseFloat(wbc) + parseFloat(crp);\n\n//interpretation\nvar prob, rec;\nif(score < 5){\n\tprob = 'Low';\n\trec = 'Outpatient follow-up (if unaltered general condition)';\n}\nif(score > 4 && score < 9){\n\tprob = 'Indeterminate';\n\trec = 'In-hospital active observation with serial re-exams, imaging, or diagnostic laparoscopy, according to local practice';\n}\nif(score > 8 && score < 13){\n\tprob = 'High';\n\trec = 'Surgical exploration recommended';\n}\n\n//results\ncalc_output.push({\n    name: 'mini',\n    value: score,\n    value_text: 'points',\n    message: prob\n});\ncalc_output.push({\n    name: 'Score',\n    value: score,\n    value_text: 'points',\n    message: 'AIR Score'\n});\ncalc_output.push({\n    name: 'interpretation',\n    value: prob,\n    value_text: 'risk',\n    message: rec\n});",
